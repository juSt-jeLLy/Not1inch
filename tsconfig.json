{
  "extends": "@1inch/tsconfig",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Bundler",
    "moduleDetection": "auto",
    "outDir": "./dist",
    "rootDir": "./s",
    "lib": ["ES2020", "DOM"],
    "target": "ES2020",
    "removeComments": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src"]
}
